{
  "version": 2,
  "routes": [
    {
      "src": "/api/feed",
      "dest": "/api/feed.js"
    },
    { "src": "/(.*)", "dest": "/public/index.html" ,
      "headers": {
        "Cache-Control": "s-maxage=1, stale-while-revalidate=59"
      }
    }
  ],
  "builds": [
    { "src": "api/feed.js", "use": "@vercel/node" },
    { "src": "public/.*", "use": "@vercel/static" }
  ]
}