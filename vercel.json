{
  "version": 2,
  "routes": [
    {
      "src": "/api/feed",
      "dest": "/api/feed.js"
    },
    { "src": "/(.*)", "dest": "/public/index.html" ,
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ],
  "builds": [
    {
      "src": "api/feed.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/.*",
      "use": "@vercel/static"
    }
  ]
}